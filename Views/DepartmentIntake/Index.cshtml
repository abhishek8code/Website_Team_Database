@model IEnumerable<GECPATAN_FACULTY_PORTAL.Models.Department.DepartmentIntake>

@{
    ViewData["Title"] = "DepartmentIntake List";
}

<h2>DepartmentIntake List</h2>

<p><a asp-action="Create" class="btn btn-primary">Create New</a></p>

<table class="table">
    <thead>
        <tr>
            <th>IntakeCount</th>
            <th>IntakeYear</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.IntakeCount</td>
            <td>@item.IntakeYear</td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
    }
    </tbody>
</table>
